{
	"name": "@ylide/everscale",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"version": "0.5.0",
	"description": "Ylide Protocol SDK implementation for EverScale blockchain",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/ylide-io/everscale.git"
	},
	"files": [
		"lib/**/*",
		"src/**/*"
	],
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 0",
		"build": "tsc",
		"start": "ts-node ./src/index.ts",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "tslint -p tsconfig.json",
		"prepublishOnly": "npm test && npm run lint",
		"preversion": "npm run lint"
	},
	"keywords": [
		"blockchain",
		"everscale",
		"messaging",
		"communication"
	],
	"author": "Danila Simonov",
	"license": "ISC",
	"dependencies": {
		"@ylide/sdk": "0.5.0",
		"@ylide/smart-buffer": "0.0.9",
		"chacha": "^2.1.0",
		"everscale-inpage-provider": "^0.3.20",
		"everscale-standalone-client": "^2.0.10",
		"moment": "^2.29.4",
		"tweetnacl": "^1.0.3"
	},
	"devDependencies": {
		"prettier": "^2.7.1",
		"ts-node": "^10.8.2",
		"tslint": "^6.1.3",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^4.7.4"
	}
}
